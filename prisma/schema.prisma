generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id          String   @id @default(uuid())
  title       String
  slug        String   @unique
  category    String
  imageUrl    String
  description String?
  createdAt   DateTime @default(now())

  @@map("projects")
}
